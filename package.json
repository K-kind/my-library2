{
  "name": "my-library",
  "version": "2.2.14",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:theme": "gulp",
    "build:index": "webpack --config build/webpack.common.js",
    "build:components": "webpack --config build/webpack.component.js",
    "copy:utils": "cp -r node_modules/element-ui/lib .",
    "install": "npm run build:theme && npm run build:index"
  },
  "main": "lib/lib-main.common.js",
  "files": [
    "lib",
    "src",
    "theme",
    "vetur"
  ],
  "dependencies": {
    "@vue/composition-api": "^1.0.0-beta.18",
    "core-js": "^3.6.5",
    "element-ui": "^2.14.0",
    "material-design-icons-iconfont": "^6.1.0",
    "vue": "^2.6.11",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^4.1.0",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.8",
    "@vue/cli-service": "~4.5.0",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.12"
  },
  "vetur": {
